<!DOCTYPE html>
<html lang="en">
<head>
<meta charset="utf-8">
<meta name="viewport" content="width=device-width, initial-scale=1.0">
<title>Gaming Cafe | Menu</title>
<meta name="description" content="">

<!-- core CSS -->
<link href="../css/bootstrap.min.css" rel="stylesheet">
<link href="../css/font-awesome.min.css" rel="stylesheet">
<link href="../css/prettyPhoto.css" rel="stylesheet">
<link href="../css/main.css" rel="stylesheet">
<link href="../css/responsive.css" rel="stylesheet">

<!--[if lt IE 9]>
    <script src="js/html5shiv.js"></script>
    <script src="js/respond.min.js"></script>
    <![endif]-->
</head>
<!--/head-->

<body>
<header id="header">
  <nav class="navbar navbar-inverse" role="banner">
    <div class="container">
      <div class="navbar-header">
        <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse"> <span class="sr-only">Toggle navigation</span> <span class="icon-bar"></span> <span class="icon-bar"></span> <span class="icon-bar"></span> </button>
        <a class="navbar-brand" href="/"><i class="fa fa-bolt"></i> Gaming Cafe</a></a> </div>
      <div class="collapse navbar-collapse navbar-right">
        <ul class="nav navbar-nav">
          <li><a href="/">Home</a></li>
          <li><a href="about-us">About</a></li>
          <li><a href="menu">Menu</a></li>
          {{#if session.user}}
            {{#ifEqual session.user.userType "customer"}}
              <li><a href="reservation">Make a Reservation</a></li>
            {{/ifEqual}}
            <!-- Only Show Reservation List for staff accounts -->
            {{#ifEqual session.user.userType "staff"}}
              <li><a href="staff" class="active">Reseration List</a></li>
            {{/ifEqual}}
          {{/if}}
          <li><a href="pricing">Prices</a></li>
          <li><a href="contact-us">Contact</a></li>
          {{#if session.user}}
            <li>
              <div class="dropdown">{{session.user.username}}
                <div class="dropdown-content">
                  <a href="logout">Logout</a>
                </div>
              </div>
            </li>
          {{else}}
            <li><a href="login">Login</a></li>
          {{/if}}
        </ul>
      </div>
    </div>
    <!--/.container--> 
  </nav>
  <!--/nav--> 
  
</header>
<!--/header-->
<div class="color-border"> </div>
<section id="inner-page">
  <div class="container">
    <div class="center">
      <h2>Reservation List</h2>
      <p class="lead">All current reservations</p>
    </div>
    <div class="row">
      <div class="features">
        <div class="col-md-4 col-sm-6">
         
         
          
        </div>
        <!--/.col-md-4-->
        
        <div class="col-md-4 col-sm-6">
        {{#each reservation}}
          <label for="name">Name: </label> <input type="text" name="Name" readonly="readonly" value="{{name}}"> <br>
          <label for="email">Email: </label> <input type="text" name="Email" readonly="readonly" value="{{email}}">  <br>
          <label for="number">Phone Number: </label> <input type="text" name="Phone" readonly="readonly" value="{{phone}}"> <br>
          <label for="date">Time: </label> <input type="text" name="Time" readonly="readonly" value="{{bookFor}}">  <br>
          <label for="people">People: </label> <input type="text" name="People" readonly="readonly" value="{{custNum}}"> <br>
          <label for="cafe">Cafe: </label> <input type="text" name="Cafe" readonly="readonly" value="{{location}}"> <br>
          <label for="note">Additional Note: </label> <input type="text" name="Note" readonly="readonly" value="{{additional_note}}"> <br>
         {{else}}
          <label for="name">Name: </label> <input type="text" name="Name" readonly="readonly" value=""> <br>
          <label for="email">Email: </label> <input type="text" name="Email" readonly="readonly" value="">  <br>
          <label for="number">Phone Number: </label> <input type="text" name="Phone" readonly="readonly" value=""> <br>
          <label for="date">Time: </label> <input type="text" name="Time" readonly="readonly" value="">  <br>
          <label for="people">People: </label> <input type="text" name="People" readonly="readonly" value=""> <br>
          <label for="cafe">Cafe: </label> <input type="text" name="Cafe" readonly="readonly" value=""> <br>
          <label for="note">Additional Note: </label> <input type="text" name="Note" readonly="readonly" value=""> <br> -->
        {{/each}}
        </div>
        <!--/.col-md-4-->
        
        <div class="col-md-4 col-sm-6">
         
           
         
        </div>
        <!--/.col-md-4-->
        
        <div class="col-md-4 col-sm-6">
        
        </div>
        <!--/.col-md-4-->
        
        <div class="col-md-4 col-sm-6">
          
        </div>
        <!--/.col-md-4-->
       
        <!--/.col-md-4--> 
      </div>
      <!--/.services--> 
    </div>
    <!--/.row--> 
  </div>
  <!--/.container--> 
</section>
<!--/#inner-page-->

  <!--/.container--> 
</section>
<!--/#get-started-->
<div class="color-border"> </div>
